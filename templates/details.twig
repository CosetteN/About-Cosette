{% extends 'main.twig' %}
	

	{% block content %}
		
		<script src="//cdn.knightlab.com/libs/timeline3/latest/js/timeline.js">
		</script>

		<div id='timeline-embed' style="width: 100%; height: 600px"></div>

		<script type="text/javascript">
			           	
           	var timeline = new TL.Timeline('timeline-embed', {{ timeline }});
           	
           	window.onresize = function(event) 
           	{
           		timeline.updateDisplay();
           	}
   		</script>
	
	{% endblock content %}